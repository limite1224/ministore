		del_flag = #{delFlag,jdbcType=VARCHAR},
		d_script_name =
		#{dScriptName,jdbcType=VARCHAR},
		lon = #{lon,jdbcType=VARCHAR},
		lat =
		#{lat,jdbcType=VARCHAR}
		where device_id = #{deviceId,jdbcType=VARCHAR}
	</update>

	<select id="queryList" resultMap="BaseResultMap"
		parameterType="com.leshun.plc.bean.DeviceInfo">
		select
		<include refid="Base_Column_List" />
		from device_info
		<include refid="pageCondition" />
		<include refid="orderBy" />
		<include refid="limit" />
	</select>
	<!--查询记录总数 -->
	<select id="queryRowCount" parameterType="com.leshun.plc.bean.DeviceInfo"
		resultType="java.lang.Integer">
		<![CDATA[select count(device_id) from device_info]]>
		<include refid="pageCondition" />
	</select>
	<sql id="pageCondition">
		<where>
			<choose>
				<when test="deviceMac!=null and deviceMac!=''">
					device_mac = #{deviceMac,jdbcType=VARCHAR}
				</when>
				<when test="deviceId!=null and deviceId!=''">
					device_id = #{deviceId,jdbcType=VARCHAR}
				</when>
				<otherwise>
					<if test="deviceName != null and deviceName!=''">
						and device_name like "%${deviceName}%"
					</if>
					<if test="deviceSecret != null and deviceSecret!=''">
						and device_secret = #{deviceSecret,jdbcType=VARCHAR}
					</if>
					<if test="productName != null and productName!=''">
						and product_name like "%${productName}%"
					</if>
					<if test="productKey != null and productKey!=''">
						and product_key = #{productKey,jdbcType=VARCHAR}
					</if>
					<if test="msgType != null and msgType!=''">
						and msg_type = #{msgType,jdbcType=VARCHAR}
					</if>
					<if test="expStatus != null and expStatus!=''">
						and exp_status = #{expStatus,jdbcType=VARCHAR}
					</if>
					<if test="status != null and status!=''">
						and status = #{status,jdbcType=VARCHAR}
					</if>
					<if test="comStatus != null and comStatus!=''">
						and com_status = #{comStatus,jdbcType=VARCHAR}
					</if>
				</otherwise>
			</choose>
		</where>
	</sql>
	<sql id="orderBy">
		order by modify_time desc
	</sql>
	<sql id="limit">
		limit ${offset},${limit}
	</sql>
</mapper>